<div class="col-xs-1">
  <div class="row">
    <div class="col-xs-2">
      <input type="checkbox" />
    </div>
    <div class="col-xs-2">
      <i class="star fa fa-star{{if starred "" "-o"}}"></i>
    </div>
  </div>
</div>
<div class="col-xs-11">
  {{#each labels key="@index" as |label|}}
    <span class="label label-warning">{{label}}</span>
  {{/each}}
  <a href="#">
    {{subject}} {{id}} {{labels.length}}
  </a>
</div>
